{
  "manifest_version": 3,
  "name": "DermaScan AI",
  "version": "1.0",
  "description": "An AI-powered chatbot to help you understand skin conditions.",
  "permissions": [
    "storage",
    "activeTab"
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "/favicon-16x16.png",
      "32": "/favicon-32x32.png",
      "192": "/android-chrome-192x192.png",
      "512": "/android-chrome-512x512.png"
    }
  },
  "icons": {
    "16": "/favicon-16x16.png",
    "32": "/favicon-32x32.png",
    "192": "/android-chrome-192x192.png",
    "512": "/android-chrome-512x512.png"
  }
}
